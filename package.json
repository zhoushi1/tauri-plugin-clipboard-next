{
  "name": "tauri-plugin-clipboard-next-api",
  "version": "0.1.4",
  "author": "zhoushi1",
  "description": "Tauri 2 clipboard plugin - read/write/listen to clipboard (text/rtf/html/image/files) across Windows, macOS, Linux and iOS.",
  "keywords": [
    "tauri",
    "tauri-plugin",
    "clipboard",
    "clipboard-manger",
    "clipboard-rs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhoushi1/tauri-plugin-clipboard-next.git"
  },
  "homepage": "https://github.com/zhoushi1/tauri-plugin-clipboard-next#readme",
  "bugs": "https://github.com/zhoushi1/tauri-plugin-clipboard-next/issues",
  "license": "MIT",
  "type": "module",
  "types": "./dist-js/index.d.ts",
  "main": "./dist-js/index.cjs",
  "module": "./dist-js/index.js",
  "exports": {
    "types": "./dist-js/index.d.ts",
    "import": "./dist-js/index.js",
    "require": "./dist-js/index.cjs"
  },
  "files": [
    "dist-js",
    "package.json",
    "README.md",
    "README.zh-CN.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build",
    "pretest": "pnpm build",
    "syncVersion": "node scripts/sync-version.cjs",
    "publishCargo": "node scripts/publish-cargo.cjs",
    "release": "release-it"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.0.0",
    "release-it": "^19.2.2",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
